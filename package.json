{
  "name": "codex-rh",
  "private": false,
  "description": "Codex fork that adds plan-mode and other powerful features.",
  "type": "module",
  "bin": {
    "codex-rh": "bin/codex-rh.js"
  },
  "scripts": {
    "format": "prettier --check *.json *.md docs/*.md .github/workflows/*.yml **/*.js",
    "format:fix": "prettier --write *.json *.md docs/*.md .github/workflows/*.yml **/*.js"
  },
  "devDependencies": {
    "prettier": "^3.5.3"
  },
  "resolutions": {
    "braces": "^3.0.3",
    "micromatch": "^4.0.8",
    "semver": "^7.7.1"
  },
  "overrides": {
    "punycode": "^2.3.1"
  },
  "engines": {
    "node": ">=22",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@10.8.1",
  "version": "1.0.4",
  "files": [
    "bin",
    "codex-cli/bin",
    "codex-cli/vendor",
    "LICENSE",
    "NOTICE",
    "README.md"
  ],
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reghope/codex-rh.git"
  },
  "keywords": [
    "codex",
    "codex-rh",
    "fork",
    "plan-mode",
    "subagents"
  ],
  "author": "reghope",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/reghope/codex-rh/issues"
  },
  "homepage": "https://github.com/reghope/codex-rh#readme"
}
